# DeepSeek API配置
# 请从 https://platform.deepseek.com/api-keys 获取API密钥
DEEPSEEK_API_KEY=sk-your-api-key-here
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1
DEEPSEEK_MODEL=deepseek-chat

# 数据库配置
DATABASE_PATH=../database/accounting.db

# 查询配置
MAX_RESULTS=1000
QUERY_TIMEOUT=30
MAX_SQL_LENGTH=5000

# 缓存配置
CACHE_ENABLED=true
CACHE_MAX_SIZE=100
CACHE_TTL=3600

# 日志配置
LOG_LEVEL=INFO
LOG_FILE=nl_query.log

# 注意：请勿将包含真实API密钥的.env文件提交到版本控制